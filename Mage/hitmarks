function GhRssDivInfo(pRssOuterDivId,
                      pRssInnerDivId,
                      pHostZoneFrameId,
                      pSumOfHorizontalPaddingMarginBorder)
{
    this.rssOuterDivId = pRssOuterDivId;
    this.rssInnerDivId = pRssInnerDivId;
    this.hostZoneFrameId = pHostZoneFrameId;
    this.sumOfHorizontalPaddingMarginBorder = pSumOfHorizontalPaddingMarginBorder;
}


