function ghAddSlideShowPictures(pGroupEntid, elemSpec)
{
    

    if (ctdData == null)
    {
        ctdData = { 'elemSpec' : elemSpec };
    }
    ctdData['bypassedContentTypeDialog'] = true;
    ctdData['typeName'] = 'picture';
    ctdData['typeShortTitle'] = 'Picture';
    ctdData['typeLongTitle'] = 'Picture';
    ctdData['iconClassNameRoot'] = 'Picture';
    
    openFileUploadTitleDialog('picture',
        'Picture', pGroupEntid);    
}


