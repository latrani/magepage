        homeLinkElem.id = "root-x6947933044216991970--sep--Queens_Lake_Middle_SchoolhomeLink";
        homeLinkElem.className = "breadcrumbLink";
    }
    
    var onloadFnName = "nbnNavigationBarOnload";
    
    var pageBody = document.body;
    var func = onloadFnName + "('false')";
    
    if (pageBody.attachEvent!=undefined) // attachEvent is a deprecated IE method
    {
        pageBody.onload = appendToFunction(pageBody.onload, func);
    } else {
        addHtmlAttribute(pageBody, "onload", func +";");
    }

function appendToFunction(func, str)
{
    var nfs = "";
    if (func != null){
        var ofs = func.toString();
        var fcb = ofs.indexOf("{");
        var lcb = ofs.indexOf("}",fcb+1);
        var oss = ofs.substring(fcb + 1, lcb);
        //alert('h3=' + ofs + '||||\n' + oss);
        nfs = nbStripLeadingSemicolons(oss);
    }
	var s = nbStripLeadingSemicolons(nfs + ";" + str);
    var f = new Function(s);
    return f;
}
function nbStripLeadingSemicolons(s){
	while (s.substr(0,1) == ';' && s.length>1) {
    	s = s.substr(1);
    }
	return s;
}
