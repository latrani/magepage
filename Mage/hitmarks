    var hcjEnableSocialMediaMenu = false;

    //Build the Social Network Menu
    $(document).ready(function()
    {
        //Box Model Fix for IE Quirks Mode
        adjustBoxModelIfIEQuirksMode();

        //Disable Social Networking for Non-Friendly URLs (aka. URLs ending in .page)
        if (isNotAnEdlineFriendlyURL() || !hcjEnableSocialMediaMenu || isPrivateResource)
        {
            hideSocialNetworkMenu();
        }
        else
        {
            showSocialNetworkMenu();
            buildSocialNetworkIconsOnTheFly();
        }
    });

    if (!window.addEventListener) {
    	/* Add compatibility for MSIE8 and earlier as they did not support DOM2 */
        window.addEventListener = function (type,listener, useCapture) {
            attachEvent('on' + type, function() { listener(event); });
        };
    }
    window.addEventListener('resize', function(){
 	   if (typeof nbnClearCache == "function"){
          nbnClearCache();
       }
 	 },false);
</script>
 
<script type="text/javascript" src="http://www.edline.net/ControlsInit_new.js?version=2013b-q1-2-p29081"></script>





        <script type="text/javascript">
            function nbPotentiallyHideAllMenus()
            {
                if (overrideMenuHiding != true)
                {
                    hdrHideAllDropDownMenus();
                    nbHideMacFirefoxScrollbars();
                }
                overrideMenuHiding = false;
            }
            function nbHideMacFirefoxScrollbars()
            {
                // Perhaps this Mac Firefox bug workaround is no longer necessary or only necessary for older versions.  I have
                // disabled this to solve TT #5020, but may need to revive it for older versions.  We'll see.
                return;

                // Counter Firefox Mac's bug relative to being unable to hide the divs (leaves scroll bar visible) TT #4084
                if (isMacFirefox)
                {
                    for (divId in overflowAdjustedDivsArray)
                    {
                        if ( overflowAdjustedDivsArray[divId] == true )
                        {
                            var subMenuDiv = document.getElementById(divId);
                            subMenuDiv.style.overflow = "hidden";
                            overflowAdjustedDivsArray[divId] = false;
                        }
                    }
                }
            }
            $(document).ready(function() {
                ghOnDocumentReady(true, false);
            });
        </script>
        
