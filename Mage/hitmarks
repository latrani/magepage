    var nbIsIE9 = ua.indexOf ('MSIE 9') != -1;
    var isFirefox = ua.indexOf ('Firefox') != -1;
    var isMacFirefox = (ua.indexOf ('Macintosh') != -1) && (isFirefox);
    var nbIsChrome = ua.indexOf ('Chrome') != -1;
