function checkStale()
{

    if (document.cookie.search(/pgt=c1365540184187t/i) == null)
    {
        document.location.reload();
    }
}


function giveIFrameFocus()
{
    var iframeCtl = document.getElementById("ghSwsIFrame");
    if (iframeCtl == null)
    {
        alert("the iframe control wasn't found");
    }

    var focusMethod = iframeCtl.focus;

    if (focusMethod != null)
    {
        iframeCtl.focus();
    }
}



function onLoadHandler()
{
    

    ghDoProcessingAfterBodyLoaded(
            false,
            false,
            true,
            false,
            true,
            'Folder',
            true,
            false,
            true,
            861,
            false,
            true);

    var outerMostDivQuery = null;

    
        var nbcWidth = document.getElementById('ghOuterMostTable').offsetWidth;
        outerMostDivQuery = $("#ghOuterMostDiv");

        if (outerMostDivQuery.length)
        {
            nbcWidth = nbcWidth -
                       ( parseInt(outerMostDivQuery.css("marginLeft"),10) || 0 ) -
                       ( parseInt(outerMostDivQuery.css("marginRight"),10) || 0 ) -
                       ( parseInt(outerMostDivQuery.css("borderLeftWidth"),10) || 0 ) -
                       ( parseInt(outerMostDivQuery.css("borderRightWidth"),10) || 0 )
                    ;
        }

       setElementWidth('ed-navBar', nbcWidth + "px" );
    

    
    ghAdjustBoxListElementsForScrollbar();

    if (isMSIE)
    {
        ghOnloadPinnedDocsHandler();   // Other browsers handled earlier in ghInitPinnedDocsLoadEventHandler
    }



    if (window.document.onkeypress == undefined)
    {
      window.document.onkeypress = ghKeyPressHandler;
    }

    checkStale();
    clearFrames();
    initMainMenuBar();

    

    

    
    

    
        ghTouchMenuBarForIE();

        
        ghAdjustHeightOfCertainZones(false);

    


    
    







  

    ghSetHighlighterCornerInfo(0, "", "", "", "", "red");

    
    ghRoundCorners();
    




    
    if (ghTranslateRequested)
    {
        ghAdjustHeightOfCertainZones(ghIsSafari || ghIsFirefox);
    }




    ghAdjustContentDivHeightAndWidth(true);
    ghSlideToolbarIcons(true);


    ghDoNormalPostLoadAdjustments(false, false,
                0);

    

    if (ghTranslateRequested)
    {
        setTimeout( function() { ghDoDelayedTranslatePostLoadAdjustments('') },
                3000);
    }



    $("#" + GH_CUSTOMER_CONTENT_WRAPPER_ID).scroll(function() { ghHandleCustomerContentScroll(); } );


    if (isMSIE)
    {
        setTimeout( function() { ghInitEventHandlers(); }, 0);   
    }

    ghEnforceMinPageHeight();

    
    setTimeout(function() { ghDoOnloadCompletelyFinishedHandling(false); }, 0);

}


function ghOnResizeHandler()
{
    ghOnResizeHandlerPart2(
            false,
            false,
            0,
            true);
}





function ghRoundCorners()
{









    var dsHighlightDivQuery = null;







                    ghBoxParentBgColor["edlElement_6947934157177955540_document"] = "#ffffff";                
                    ghBoxTextColor["edlElement_6947934157177955540_document"] = "#000000";                
                
                    ghBoxParentBgColor["edlElement_6947933085810661794_folder"] = "#ffffff";                
                    ghBoxTextColor["edlElement_6947933085810661794_folder"] = "#ffffff";                
                
                    ghBoxParentBgColor["edlViewAsBoxArea"] = "#ffffff";                
                    ghBoxTextColor["edlViewAsBoxArea"] = "#000000";                
                
                    ghBoxParentBgColor["edlContentsBoxArea"] = "#ffffff";                
                    ghBoxTextColor["edlContentsBoxArea"] = "#000000";                
                
                    ghBoxParentBgColor["edlLinksBoxArea"] = "#ffffff";                
                    ghBoxTextColor["edlLinksBoxArea"] = "#000000";                
                
                    ghBoxParentBgColor["edlElement_6947934125158996756_document"] = "#ffffff";                
                    ghBoxTextColor["edlElement_6947934125158996756_document"] = "#000000";                
                
                    ghBoxParentBgColor["edlElement_6947934730732534258_folder"] = "#ffffff";                
                    ghBoxTextColor["edlElement_6947934730732534258_folder"] = "#000000";                
                
                    ghBoxParentBgColor["edlNewsBoxArea"] = "#ffffff";                
                    ghBoxTextColor["edlNewsBoxArea"] = "#000000";                
                
                    ghBoxParentBgColor["edlCalendarBoxArea"] = "#ffffff";                
                    ghBoxTextColor["edlCalendarBoxArea"] = "#000000";                
                
                    ghBoxParentBgColor["edlElement_6947933085817103492_document"] = "#ffffff";                
                    ghBoxTextColor["edlElement_6947933085817103492_document"] = "#000000";                
                
}






function ghPotentiallyWidenRssDivs()
{
    for (var key in ghRssDivsToWiden)
    {
    	var obj = ghRssDivsToWiden[key];
        var zoneElem = document.getElementById(obj.hostZoneFrameId);
        var newWidth = parseInt(zoneElem.offsetWidth,10) - obj.sumOfHorizontalPaddingMarginBorder;
        setElementWidth(obj.rssOuterDivId, newWidth);
        setElementWidth(obj.rssInnerDivId, newWidth);
    }
}

            
function ghKeyPressHandler(e)
{

  

    var escapePressed = false;

    if (isMSIE)
    {
        escapePressed  = window.event.keyCode == '27';
    }
    else
    {
        escapePressed  = e.keyCode == '27';
    }

    if (escapePressed == true)
    {
            hdrHideAllDropDownMenus();
    }

  

}


function ghHideOrShowBoxContents(pContentsBoxId, pHideOrShow, pShowBoxContentsImagePath, pHideBoxContentsImagePath,
                                 pIsHeaderIconShownAndExpandOnHoverSelected, pHasARoundedCorner, pBoxElementDivId,
                                 pZoneFrameDivId, pBoxListHeight)
{
  var contentsDiv = document.getElementById(pContentsBoxId);
  var hideOrShowImg = document.getElementById(pContentsBoxId + 'Image');

  var boxElementDivQuery = $('#' + pBoxElementDivId);

  if (pHideOrShow == "hide")
  {
      
      boxElementDivQuery.find('div.edlBoxTitle').css('margin-bottom', '0px');

      
      if (isMSIE)
      {
          boxElementDivQuery.css('position', '');
      }

      contentsDiv.style.display = "none";
      hideOrShowImg.setAttribute("src", pShowBoxContentsImagePath);
      hideOrShowImg.setAttribute("title", "Show/expand box contents");

      // TODO sms 10/27/10: construct differently so that a literal isn't used so that ghHideOrShowBoxContents usage searches can be more complete
      hideOrShowImg.onclick = new Function("javascript:ghHideOrShowBoxContents('"
              + pContentsBoxId + "','show','" + pShowBoxContentsImagePath + "','" +
                pHideBoxContentsImagePath + "', " + pIsHeaderIconShownAndExpandOnHoverSelected + "," +
                pHasARoundedCorner + ",'" + pBoxElementDivId + "','" + pZoneFrameDivId + "'," + pBoxListHeight + ")");

      if (pIsHeaderIconShownAndExpandOnHoverSelected)
      {
          hideOrShowImg.style.display = "none";
      }
      else
      {
          hideOrShowImg.style.display = "inline";
      }

  }
  else
  {
      
      if (pBoxListHeight == 0)
      {
          boxElementDivQuery.find('div.edlBoxTitle').css('margin-bottom', '5px');
      }

      
      if (isMSIE)
      {
          boxElementDivQuery.css('position', 'static');
      }

      contentsDiv.style.display = "block";   
      hideOrShowImg.setAttribute("src", pHideBoxContentsImagePath);
      hideOrShowImg.setAttribute("title", "Hide/collapse box contents");

      hideOrShowImg.onclick = new Function("javascript:ghHideOrShowBoxContents('"
              + pContentsBoxId + "','hide','" + pShowBoxContentsImagePath + "','" +
                pHideBoxContentsImagePath + "', " + pIsHeaderIconShownAndExpandOnHoverSelected + "," +
                pHasARoundedCorner + ",'" + pBoxElementDivId + "','" + pZoneFrameDivId + "'," + pBoxListHeight + ")");

      hideOrShowImg.style.display = "inline";
  }

    
    if (ghIsFirefox || ghIsSafari)
    {
        $('#' + pZoneFrameDivId).css("height","auto");
    }

    if(isMSIE){
	    if (pHasARoundedCorner)
	    {
	        boxElementDivQuery.css("height","auto");
	        // ghRoundCornersPrepBoxForIe(pBoxElementDivId);
	    }
        ghRoundCorners();

	    
	    ghTouchMenuBarForIE();
    }
  ghRequestBoxExpandCollapseTracking(pContentsBoxId, pHideOrShow);
}


function ghRequestBoxExpandCollapseTracking(pContentsBoxId, pHideOrShow)
{

    var parameters = "contentsBoxId=" + pContentsBoxId + "&hideOrShow=" + pHideOrShow;

    $.ajax({
        type: "POST",
        url: "/BoxCollapseExpand.page",
        async: false,
        data: parameters,
        complete: function(xmlRequestObject, successString){
            ghReceiveBoxExpandCollapseTracking(xmlRequestObject, successString);
        }
    });

}


function ghReceiveBoxExpandCollapseTracking(ajaxReq, successString)
{
    
}

function ghShowPageNotFound()
{
    var dlg = $("<div title='Page Not Found'>" +
                    "<p>Whoops, this page is no longer available!</p>" +
                "</div>");
    var opt = {
        resizable: false,
        draggable: false,
        modal: true,
        dialogClass: "edline-dialog",
        buttons: {
            "OK" : function() {
                dlg.dialog("close");
                dlg.dialog("destroy");
                dlg.remove();
            }
        },
        width: 300,
        height: "auto",
        open: function() {
            $('.ui-dialog-buttonpane').find('button:contains("OK")').addClass('primary');
        }
    };

    if ($.fn.dialog == null)
    {
        $.getScript("/jscripts/jquery/jquery-ui.js", function() {
            dlg.dialog(opt);
        });
    }
    else
    {
        dlg.dialog(opt);
    }
}






</script>
