<script type="text/javascript">


    function translateMenuOver(pEvent)
    {
        overrideMenuHiding = true;
        showTranslateMenu(pEvent);
    }


    function translateMenuOut(pEvent)
    {
    }


    function showTranslateMenu(pEvent)
    {
      // Hide the navbar menus if they are open:
      hdrHideAllDropDownMenus();

      var linkElem = document.getElementById("edlLangLink");
      var linkElemOffset = $("#edlLangLink").offset();
      var linkLeftEdge = linkElemOffset.left;
      var linkBottomEdge = linkElemOffset.top + linkElem.offsetHeight;

      translateMenu.moveTo(linkLeftEdge, linkBottomEdge + 1);
      translateMenu.show();
    }

    function switchLang(lang)
    {
        if (translationDisabledMessage())
        {
            return;
        }

        submitEvent('language', 'TCNK=headerComponent;language='+lang);
    }


</script>












